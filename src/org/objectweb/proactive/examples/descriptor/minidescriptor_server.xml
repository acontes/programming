<?xml version="1.0" encoding="UTF-8"?>
<ProActiveDescriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DescriptorSchema.xsd">
	<componentDefinition>
	   <virtualNodesDefinition>
	     <virtualNode name="MiniVNServer"/>
	   </virtualNodesDefinition>
	</componentDefinition>
	<deployment>
		<register virtualNode="MiniVNServer" protocol="rmi"/>
		<mapping>
			<map virtualNode="MiniVNServer">
				<jvmSet>
					<currentJVM protocol="rmi"/>
				</jvmSet>
			</map>
		</mapping>
	</deployment>


	<infrastructure>
		<processes>
			<processDefinition id="PROCESSlocal">
				<jvmProcess class="org.objectweb.proactive.core.process.JVMNodeProcess"/>
			</processDefinition>
		</processes>
	</infrastructure>


</ProActiveDescriptor>
