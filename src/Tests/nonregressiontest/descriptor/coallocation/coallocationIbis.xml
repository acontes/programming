<?xml version="1.0" encoding="UTF-8"?>
<ProActiveDescriptor schemaVersion="3.3" xmlns="http://www-sop.inria.fr/oasis/ProActive/schemas/DescriptorSchema.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www-sop.inria.fr/oasis/ProActive/schemas/DescriptorSchema.xsd">
	<componentDefinition>
		<virtualNodesDefinition>
			<virtualNode name="covn1"/>
			<virtualNode name="covn2"/>
		</virtualNodesDefinition>
	</componentDefinition>
	<deployment>
		<mapping>
			<map virtualNode="covn1">
				<jvmSet>
					<vmName value="Jvm1"/>
				</jvmSet>
			</map>
			<map virtualNode="covn2">
				<jvmSet>
					<vmName value="Jvm1"/>
				</jvmSet>
			</map>
		</mapping>
		<jvms>
			<jvm name="Jvm1">
				<creation>
					<processReference refid="linuxJVM"/>
				</creation>
			</jvm>
		</jvms>
	</deployment>
	<infrastructure>
		<processes>
			<processDefinition id="linuxJVM">
	           <jvmProcess class="org.objectweb.proactive.core.process.JVMNodeProcess">
					<jvmParameters>
						<parameter value="-Dproactive.future.ac=enable"/>
						<parameter value="-Dproactive.communication.protocol=ibis"/>
						<parameter value="-Dibis.serialization=ibis"/>
						<parameter value="-Dibis.name_server.host=localhost"/>
						<parameter value="-Dibis.name_server.pool=rutget"/>
						<parameter value="-Dibis.name_server.key=12"/>
					</jvmParameters>
				</jvmProcess>
				</processDefinition>
		</processes>
	</infrastructure>
</ProActiveDescriptor>
