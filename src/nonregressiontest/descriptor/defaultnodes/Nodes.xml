<?xml version="1.0" encoding="UTF-8"?>
<ProActiveDescriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../descriptors/DescriptorSchema.xsd">
	<componentDefinition>
		<virtualNodesDefinition>
			<virtualNode name="Dispatcher"/>
			<virtualNode name="Dispatcher1"/>
			<virtualNode name="Dispatcher2"/>
			<virtualNode name="Dispatcher3-AC"/>
			<virtualNode name="Cyclic-AC"/>
		</virtualNodesDefinition>
	</componentDefinition>
	<deployment>
		<mapping>
			<map virtualNode="Dispatcher">
				<jvmSet>
					<currentJVM protocol="http"/>
				</jvmSet>
			</map>
			<map virtualNode="Dispatcher1">
				<jvmSet>
					<vmName value="Jvm1"/>
				</jvmSet>
			</map>
			<map virtualNode="Dispatcher2">
				<jvmSet>
					<vmName value="Jvm2"/>
				</jvmSet>
			</map>
			<map virtualNode="Dispatcher3-AC">
				<jvmSet>
					<vmName value="Jvm3-AC"/>
				</jvmSet>
			</map>
			<map virtualNode="Cyclic-AC">
				<jvmSet>
					<vmName value="Jvm3-AC"/>
					<vmName value="Jvm4-AC"/>
				</jvmSet>
			</map>
		</mapping>
		<jvms>
			<jvm name="Jvm1">
				<creation>
					<processReference refid="linuxJVM"/>
				</creation>
			</jvm>
			<jvm name="Jvm2">
				<creation>
					<processReference refid="rshProcess"/>
				</creation>
			</jvm>
			<jvm name="Jvm3-AC">
				<creation>
					<processReference refid="rshProcess-AC"/>
				</creation>
			</jvm>
			<jvm name="Jvm4-AC">
				<creation>
					<processReference refid="rshProcess-AC"/>
				</creation>
			</jvm>
		</jvms>
	</deployment>
	<infrastructure>
		<processes>
			<processDefinition id="linuxJVM">
				<jvmProcess class="org.objectweb.proactive.core.process.JVMNodeProcess"/>
			</processDefinition>
			<processDefinition id="linuxJVM-AC">
				<jvmProcess class="org.objectweb.proactive.core.process.JVMNodeProcess">
					<jvmParameters>
						<parameter value="-Dproactive.future.ac=enable"/>
					</jvmParameters>
				</jvmProcess>
			</processDefinition>
			<processDefinition id="rshProcess">
				<rshProcess class="org.objectweb.proactive.core.process.rsh.RSHJVMProcess" hostname="sea">
					<processReference refid="linuxJVM"/>
				</rshProcess>
			</processDefinition>
			<processDefinition id="rshProcess-AC">
				<rshProcess class="org.objectweb.proactive.core.process.rsh.RSHJVMProcess" hostname="nahuel">
					<processReference refid="linuxJVM-AC"/>
				</rshProcess>
			</processDefinition>
		</processes>
	</infrastructure>
</ProActiveDescriptor>
