xsi:schemaLocation="urn:gcm:deployment:1.0 http://proactive.inria.fr/schemas/gcm/1.0/ExtensionSchemas.xsd	">

	<environment>
		<javaPropertyVariable name="user.home" />
                <javaPropertyVariable name="java.home" />
		<descriptorVariable name="cheypa.home" value="/home/akapur"/>
	</environment>


	<resources>
	     <group refid="sshLan">
                    <host refid="ComputeNode" />
             </group>
	</resources>

<infrastructure>

    	<hosts>
	       <host id="ComputeNode" os="unix">
		   <homeDirectory relpath="${cheypa.home}"/>
	       </host>

       </hosts>

       <groups>
            <sshGroup id="sshLan" hostList="cheypa cheypa" />
       </groups> 

</infrastructure>
</GCMDeployment>
