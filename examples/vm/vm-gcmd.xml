<?xml version="1.0" encoding="UTF-8"?>
<GCMDeployment xmlns="urn:gcm:deployment:1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:gcm:deployment:1.0 http://proactive.inria.fr/schemas/gcm/1.0/ExtensionSchemas.xsd">

	<environment>
		<javaPropertyVariable name="user.home" />
	</environment>

	<resources>
		<host refid="localh"/>
		<group refid="activeeong">
			<host refid="localh"/>
		</group>
	</resources>

	<infrastructure>
		<hosts>
			<host os="unix" id="localh">
				<homeDirectory base="root" relpath="${user.home}"/>
			</host>
			<host os="unix" id="nefh">
				<homeDirectory base="root" relpath="/export/home/jmguilla"/>
				<tool id="proactive" path="/export/home/jmguilla/workspace/ProActive"/>
			</host>
		</hosts>
		<bridges>
			<sshBridge hostname="nef.inria.fr" id="nefb" />
		</bridges>
		<groups>
			<pbsGroup bookedNodesAccess="ssh" id="nefg">
				<resources nodes="3" walltime="00:10:00" />
				<stdout>/export/home/jmguilla/vm.stdout</stdout>
				<stderr>/export/home/jmguilla/vm.stderr</stderr>
			</pbsGroup>
			<sshGroup hostList="excalibur.activeeon.com jily.activeeon.com" id="activeeong" />
		</groups>
	</infrastructure>
</GCMDeployment>