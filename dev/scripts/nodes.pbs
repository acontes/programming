#!/bin/bash
#PBS -l nodes=1:ppn=1,walltime=00:00:30

 mkdir -p ~/.cluster
 GMCONF=~/.cluster/mconf.$PBS_JOBID
 uniq < $PBS_NODEFILE > $GMCONF
 NODES=$GMCONF

echo "Node file " $NODES
echo "Program " $PRUN_PROG
echo $PRUN_PROGARGS
for d in `cat $NODES`
do
  echo "//$d.das2.cs.vu.nl/Node1"
  rsh $d hostname
done
