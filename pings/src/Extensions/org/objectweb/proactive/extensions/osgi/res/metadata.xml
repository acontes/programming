<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  
  
  <component class="org.objectweb.proactive.osgi.ProActiveServicesImpl">
	<provides
		service="org.objectweb.proactive.osgi.ProActiveService"/>
	<property name="version" value="1.0.0" type="string"/>		
	<requires
        service="org.osgi.service.http.HttpService"
        cardinality="1..1"
		filter=""
        policy="dynamic" 
        bind-method="bind"
        unbind-method="unbind"
    />
  </component>
  
  <component class="org.objectweb.proactive.osgi.Activator">	

    <requires
    	service="org.objectweb.proactive.osgi.ProActiveService" 
    	cardinality="1..n"
    	filter=""
    	policy="dynamic"
    	bind-method="bindProActiveService"
    	unbind-method="unbindProActiveService"
    />
  </component>
  
	
  
</bundle>  