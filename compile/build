#!/bin/bash
#build file for Unix system
currentDir=`dirname $0`
if [ $# -lt 1 ]
then
  args="-projecthelp"
else
  args=$@
fi
$JAVA_HOME/bin/java -Xmx256000000 -classpath $JAVA_HOME/lib/tools.jar:$currentDir/ant.jar:$currentDir/ant-launcher.jar:$currentDir/xercesImpl.jar:$currentDir/xml-apis.jar:$CLASSPATH org.apache.tools.ant.Main -emacs -buildfile $currentDir/proactive.xml $args
